{
  "name": "react-redux-webpack",
  "version": "1.0.0",
  "description": "react-redux-webpack-base-setup",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean:build": "rimraf build",
    "prebuild:dev": "npm run clean:build",
    "build:dev": "webpack --config webpack.dev.config.js --progress --profile --colors --display-error-details",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js --hot --progress --profile --colors --display-error-details --content-base app/",
    "start": "npm run server:dev",
    "test": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wangleiwan/react-redux-webpack.git"
  },
  "author": "Lei Wang",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "copy-webpack-plugin": "1.1.1",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "html-webpack-plugin": "2.14.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-grid-layout": "0.11.1",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "redux": "3.3.1",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.4",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
